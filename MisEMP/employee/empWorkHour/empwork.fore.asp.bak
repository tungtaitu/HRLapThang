<%
'on error resume next
session.codepage="65001"
SELF = "empfileedit"

Set conn = GetSQLServerConnection()
Set rs = Server.CreateObject("ADODB.Recordset")

yymmstr = request("yymm")
country = request("country")
whsno = trim(request("whsno"))
unitno = trim(request("unitno"))
groupid = trim(request("groupid"))
zuno = trim(request("zuno"))
job = trim(request("job"))
QUERYX = trim(request("empid1"))
shift = request("shift")


gTotalPage = 1
PageRec = 10    'number of records per page
TableRec = 50    'number of fields per record
'if day(date())<=10 then
'	if month(date()) = 12 then
'		yymm = year(date())- 1 & right("00" & month(date()) , 2 )
'	else
'		yymm = year(date())& right("00" & month(date())-1 , 2 )
'	end if
'else
'	yymm = year(date())& right("00" & month(date()), 2 )
'end if
yymm = yymmstr  
nowmonth = year(date())&right("00"&month(date()),2) 

calcdt = left(YYMM,4)&"/"& right(yymm,2)&"/01"

if right(yymmstr,2) mod 2 = 0 then 
	ccx=35
else
	ccx=36
end if 		

if session("netuser")="LSARY" and yymmstr < nowmonth then  
	sql="select * from   "&_
		"(select  empid  WORKEMPID ,(toth+h1+h2) as Tj, (h1+h2)-"& cdbl(ccx)&" as OverJ, 0 as forget, 0 as latefor,  *  "&_
		"from  empworkper  where  yymm='"& yymmstr &"'  ) a "&_
		"LEFT JOIN   View_EMPFILE  B ON B.EMPID =A.WORKEMPID   "&_ 
		"left join ( select  empid  WORKEMPIDx , sum( isnull(b3,0) ) b3 from EMPWORK  WHERE  YYMM='"& yymmstr &"'  group by empid   "&_
		") c on c.WORKEMPIDx = a.WORKEMPID "&_
		"where isnull(b.status,'')<>'D' and (isnull(b.outdate,'')='' or b.outdate>'"& calcdt &"'  )  "&_
		"and  whsno like '%"& whsno &"%' and unitno like '%"& unitno &"%'  and b.groupid like '%"& groupid &"%'  "&_
		"and country like '%"& country &"%' and job like '%"& job &"%' and b.empid like '%"& QUERYX &"%' "
else 
	sql=" SELECT  (h1+h2)-"& cdbl(ccx)&" as OverJ,  toth as tj, *  FROM   "&_
		"(   select  empid  WORKEMPID ,  sum(isnull(toth,0)) toth, sum(isnull( forget ,0 )) forget, "&_
		"sum( isnull(  latefor,0) ) latefor  , sum( isnull( kzhour,0) ) kzhour , sum( isnull( h1,0) ) h1,  "&_
		"sum( isnull( h2,0) ) h2,  sum( isnull( h3,0) ) h3 , sum( isnull(b3,0) ) b3 , "&_
		"sum( isnull(jiaa,0) ) jiaA, sum( isnull(jiaB,0) ) jiaB, sum( isnull(jiaC,0) ) jiaC, "&_
		"sum( isnull(jiaD,0) ) jiaD, sum( isnull(jiaE,0) ) jiaE, sum( isnull(jiaF,0) ) jiaF, "&_
		"sum( isnull(jiaG,0) ) jiaG, sum( isnull(jiaH,0) ) jiaH  "&_
		"from   EMPWORK  WHERE  YYMM='"& yymmstr &"'  group by empid   "&_
		") A "&_
		"LEFT JOIN   View_EMPFILE  B ON B.EMPID =A.WORKEMPID   "&_
		"where isnull(b.status,'')<>'D' and (isnull(b.outdate,'')='' or b.outdate>'"& calcdt &"'  )  "&_
		"and  whsno like '%"& whsno &"%' and unitno like '%"& unitno &"%'  and b.groupid like '%"& groupid &"%'  "&_
		"and country like '%"& country &"%' and job like '%"& job &"%' and b.empid like '%"& QUERYX &"%' "
end if 

if shift<>"" and shift<>"D" then
	sql=sql&"and  b.shift='"& shift &"' "
end if
if shift="D" then
	sql=sql&"and  isnull(b.shift,'')='' "
end if
sql=sql&"order by b.empid "
	
response.write sql
'response.end
if request("TotalPage") = "" or request("TotalPage") = "0" then
	CurrentPage = 1
	rs.Open SQL, conn, 3, 3
	IF NOT RS.EOF THEN
		rs.PageSize = PageRec
		RecordInDB = rs.RecordCount
		TotalPage = rs.PageCount
		gTotalPage = TotalPage
	END IF

	Redim tmpRec(gTotalPage, PageRec, TableRec)   'Array

	for i = 1 to TotalPage
	 for j = 1 to PageRec
		if not rs.EOF then
			for k=1 to TableRec-1
				tmpRec(i, j, 0) = "no"
				tmpRec(i, j, 1) = trim(rs("empid"))
				tmpRec(i, j, 2) = trim(rs("empnam_cn"))
				tmpRec(i, j, 3) = trim(rs("empnam_vn"))
				tmpRec(i, j, 4) = rs("country")
				tmpRec(i, j, 5) = rs("nindat")
				tmpRec(i, j, 6) = rs("job")
				tmpRec(i, j, 7) = rs("whsno")
				tmpRec(i, j, 8) = rs("unitno")
				tmpRec(i, j, 9)	=RS("groupid")
				tmpRec(i, j, 10)=RS("zuno")
				tmpRec(i, j, 11)=RS("wstr")
				tmpRec(i, j, 12)=RS("ustr")
				tmpRec(i, j, 13)=RS("gstr")
				tmpRec(i, j, 14)=RS("zstr")
				tmpRec(i, j, 15)=RS("jstr")
				tmpRec(i, j, 16)=RS("cstr")
				tmpRec(i, j, 17)=RS("autoid")
				IF RS("zuno")="XX" THEN
					tmpRec(i, j, 18)=""
				ELSE
					tmpRec(i, j, 18)=RS("zuno")
				END IF
				if session("netuser")="LSARY" then    				
					if cdbl(rs("overJ"))> 0 then 
						tmpRec(i, j, 19)=(cdbl(RS("tj")))-cdbl(rs("overJ"))
					else
						tmpRec(i, j, 19)=cdbl(RS("toth"))+cdbl(RS("h1"))+cdbl(RS("h2"))
					end if 	
					tmpRec(i, j, 20)=cdbl(RS("forget"))
					tmpRec(i, j, 21)=cdbl(RS("latefor"))
					tmpRec(i, j, 22)=cdbl(RS("kzhour")) 
					if  cdbl(rs("overJ"))> 0 then 
						tmpRec(i, j, 23)=cdbl(RS("h1"))- cdbl(rs("overJ"))
					else
						tmpRec(i, j, 23)=cdbl(RS("h1"))
					end if 	
					tmpRec(i, j, 24)=cdbl(RS("h2"))
					tmpRec(i, j, 25)=cdbl(RS("h3"))
					tmpRec(i, j, 26)="0"
					tmpRec(i, j, 27)=cdbl(RS("jiaa"))
					tmpRec(i, j, 28)=cdbl(RS("jiab"))
					tmpRec(i, j, 29)=cdbl(RS("jiac"))
					tmpRec(i, j, 30)=cdbl(RS("jiad"))
					tmpRec(i, j, 31)=cdbl(RS("jiae"))
					tmpRec(i, j, 32)=cdbl(RS("jiaf"))
					tmpRec(i, j, 33)=cdbl(RS("jiag"))
					tmpRec(i, j, 34)=cdbl(RS("jiah")) 
				else
					tmpRec(i, j, 19)=cdbl(RS("totH"))
					tmpRec(i, j, 20)=cdbl(RS("forget"))
					tmpRec(i, j, 21)=cdbl(RS("latefor"))
					tmpRec(i, j, 22)=cdbl(RS("kzhour"))
					tmpRec(i, j, 23)=cdbl(RS("h1"))
					tmpRec(i, j, 24)=cdbl(RS("h2"))
					tmpRec(i, j, 25)=cdbl(RS("h3"))
					tmpRec(i, j, 26)=cdbl(RS("b3"))
					tmpRec(i, j, 27)=cdbl(RS("jiaa"))
					tmpRec(i, j, 28)=cdbl(RS("jiab"))
					tmpRec(i, j, 29)=cdbl(RS("jiac"))
					tmpRec(i, j, 30)=cdbl(RS("jiad"))
					tmpRec(i, j, 31)=cdbl(RS("jiae"))
					tmpRec(i, j, 32)=cdbl(RS("jiaf"))
					tmpRec(i, j, 33)=cdbl(RS("jiag"))
					tmpRec(i, j, 34)=cdbl(RS("jiah")) 				
				end if 	
				tmpRec(i, j, 35)=trim(RS("outdate"))

			next
			rs.MoveNext
		else
			exit for
		end if
	 next

	 if rs.EOF then
		rs.Close
		Set rs = nothing
		exit for
	 end if
	next
	Session("empfileeditN") = tmpRec
else
	TotalPage = cint(request("TotalPage"))
	'StoreToSession()
	CurrentPage = cint(request("CurrentPage"))
	RecordInDB  = REQUEST("RecordInDB")
	tmpRec = Session("empfileeditN")

	Select case request("send")
	     Case "FIRST"
		      CurrentPage = 1
	     Case "BACK"
		      if cint(CurrentPage) <> 1 then
			     CurrentPage = CurrentPage - 1
		      end if
	     Case "NEXT"
		      if cint(CurrentPage) <= cint(TotalPage) then
			     CurrentPage = CurrentPage + 1
		      end if
	     Case "END"
		      CurrentPage = TotalPage
	     Case Else
		      CurrentPage = 1
	end Select
end if


